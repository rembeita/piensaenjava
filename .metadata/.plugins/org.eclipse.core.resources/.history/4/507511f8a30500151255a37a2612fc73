
public class Ejercicio4 {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 boolean primo = false;
		
			for (int i=1; i <= 1000; i++)
			{
				primo = true;
				for (int j=2; j < i; j++)
				{
					if (i%j==0)
					{
						
						primo=false;		
					}
					System.out.println(i + " " + j);
				}
				
				if (primo)
			            System.out.print(i+" ");
			}
	}

}
